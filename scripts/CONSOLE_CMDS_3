# Console Commands for Step 3 - Testing
# Only edit the section between CODE START and CODE END

### CODE START ###
# Initialize core systems
/run scripts/path_config.py
/run scripts/init_logging.py

# Setup test environment
/run scripts/setup_test_env.py
/run scripts/install_test_dependencies.py

# Initialize test database and admin
/run scripts/startup/init_db.py --validate-schema
/run scripts/startup/init_admin.py --verify-email --check-password

# Run all test suites
/run scripts/run_tests.py --unit
/run scripts/run_tests.py --integration
/run scripts/run_tests.py --e2e

# Generate coverage reports
/run scripts/generate_coverage_report.py
/run scripts/verify_test_coverage.py --min-coverage=80

# Verify test cleanup
/run scripts/verify_test_cleanup.py

# Final verification
/run scripts/verify_tests.py
/run scripts/verify_deployment.py

### CODE END ###

# Proceed to Step 4 - Review
/load scripts/STEP_4
