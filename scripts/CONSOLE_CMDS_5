# Console Commands for Step 5 - Document, Demo and Deploy
# Only edit the section between CODE START and CODE END

### CODE START ###
# Initialize core systems
/run scripts/path_config.py
/run scripts/init_logging.py

# Finalize documentation
/run scripts/finalize_docs.py --strict --validate

# Run final verification
/run scripts/verify_deployment.py --final-check --validate-all
/run scripts/verify_render_ready.py --strict --validate-vars

# Prepare for deployment
/run scripts/backup_rotation.py --max-backups=5 --compress
/run scripts/final_cleanup.py

# Push to GitHub
/run git add .
/run git commit -m "Final deployment preparation"
/run git push origin main

# Deploy to Render
/run scripts/deploy_to_render.py

### CODE END ###

# Proceed to Final Cleanup
/load scripts/FINAL_CLEANUP
