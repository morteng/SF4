# Console Commands for Step 5 - Document and Deploy
# IMPORTANT! ONLY edit the section between CODE START and CODE END

### CODE START ###
# Initialize logging system
/run scripts/init_logging.py

# Create final backup
/run scripts/create_backup.py --timestamped

# Verify backup integrity
/run scripts/verify_backup.py --full

# Archive logs
/run scripts/archive_logs.py --force

# Verify log archives
/run scripts/verify_logs.py --strict

# Verify deployment configuration
/run scripts/verify_deployment.py --full

# Generate final documentation
/run scripts/finalize_docs.py --overwrite

# Push changes to GitHub
/run git add .
/run git commit -m "Final deployment v1.2.11"
/run git push origin main

# Deploy to production
/run scripts/deploy_to_render.py --confirm

# Verify successful deployment
/run scripts/verify_deployment.py --post-deploy

# Perform final cleanup
/run scripts/final_cleanup.py --all

# End development cycle
/run scripts/end_cycle.py --log

### CODE END ###

# Reset and start new cycle
/reset
/load scripts/STEP_1
