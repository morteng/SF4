# Console Commands for Step 5 - Document and Deploy
# IMPORTANT! ONLY edit the section between CODE START and CODE END

### CODE START ###
# Verify git state
/run scripts/verify_git_state.py --pull --retry=3

# Create final backup
/run scripts/create_backup.py --timestamped
/run scripts/verify_backup.py

# Archive logs
/run scripts/archive_logs.py --force
/run scripts/verify_logs.py

# Finalize documentation
/run scripts/finalize_docs.py

# Generate deployment checklist
/run scripts/generate_deployment_checklist.py

# Verify deployment readiness
/run scripts/verify_deployment.py --full

# Deploy to Render
/run scripts/deploy_to_render.py

# Push to GitHub
/run git push origin main

# Final cleanup
/run scripts/final_cleanup.py
/run scripts/end_cycle.py

# Verify git state after deployment
/run scripts/verify_git_state.py --retry=3
### CODE END ###

# Go to final cleanup
/load scripts/FINAL_CLEANUP
