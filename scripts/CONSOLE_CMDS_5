# Console Commands for Step 5 - Document, Demo and Deploy
# Only edit the section between CODE START and CODE END

### CODE START ###
# Initialize core systems
/run scripts/path_config.py
/run scripts/init_logging.py

# Fix critical security issues
/run scripts/fix_security_permissions.py --strict --verify --reset

# Finalize documentation with coverage
/run scripts/finalize_docs.py --strict --validate --with-coverage --critical

# Generate deployment checklist
/run scripts/generate_deployment_checklist.py --validate --strict --critical

# Verify deployment requirements
/run scripts/verify_deployment_requirements.py --check-env --validate-keys --check-production

# Verify security settings
/run scripts/verify_security.py --check-env --validate-keys --verify-permissions --check-rotation

# Verify git state
/run scripts/verify_git_state.py --pull --retry=3 --auto-resolve

# Create fresh backup
/run scripts/backup_rotation.py --max-backups=5 --compress --verify

# Request final approval
/run scripts/request_approval.py --final --validate --critical

# Deploy to production
/run scripts/deploy_to_render.py --final --validate --critical

# Perform final cleanup
/run scripts/final_cleanup.py --strict --validate --reset

### CODE END ###

# Reset and start new cycle
/load scripts/FINAL_CLEANUP
