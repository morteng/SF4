# Console Commands for Step 5 - Document, Demo and Deploy
# Only edit the section between CODE START and CODE END

### CODE START ###
# Initialize core systems
/run scripts/path_config.py
/run scripts/init_logging.py

# Finalize documentation with Render details
/run scripts/finalize_docs.py --strict --validate --render

# Verify deployment readiness
/run scripts/verify_deployment.py --final-check --validate-all

# Verify git state and commit changes
/run scripts/verify_git_state.py --pull --retry=3 --auto-resolve

# Create fresh backup before deployment
/run scripts/backup_rotation.py --max-backups=5 --compress --verify

# Verify Render configuration
/run scripts/verify_render_ready.py --check-api --validate-vars --test-deploy

# Deploy to Render
/run scripts/deploy_to_render.py

# Final cleanup
/run scripts/final_cleanup.py --strict --validate

### CODE END ###

# Reset and start new cycle
/load scripts/FINAL_CLEANUP
